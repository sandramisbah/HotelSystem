<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="CENTER" hgap="10" prefHeight="500" prefWidth="600" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="global.sample.IDSearch">
   <children>
       <TableView fx:id="CustomerTableView" prefHeight="58.0" prefWidth="335.0" GridPane.columnIndex="2" GridPane.columnSpan="4" GridPane.rowIndex="7">
       <columns>
           <TableColumn fx:id="Booking_ID" prefWidth="105.0" text="BookingID" />
           <TableColumn fx:id="Start_date" prefWidth="126.0" text="StartDate" />
           <TableColumn fx:id="End_date" prefWidth="103.0" text="EndDate" />
       </columns>
       </TableView>
       <Label text="Enter BookingID" GridPane.columnIndex="1" />
       <TextField fx:id="search" GridPane.columnIndex="2" />
   
       <Button onAction="#getCustomerlist" text="Search" GridPane.columnIndex="3" />
   
       <Button text="Modify Booking" GridPane.columnIndex="2" GridPane.rowIndex="3" />
       <Button onAction="#CancelBooking" text="Cancel Booking" GridPane.columnIndex="2" GridPane.rowIndex="4" />
       <Button text="Save Changes" GridPane.columnIndex="4" GridPane.rowIndex="8" />
      <Button fx:id="back9" mnemonicParsing="false" text="Back" GridPane.rowIndex="9" onAction="#CBack" />
